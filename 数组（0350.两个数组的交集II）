class Solution {
public:
    vector<int> intersect(vector<int>& nums1, vector<int>& nums2) {
        if(nums1.size() > nums2.size())
        return intersect(nums2, nums1);
        unordered_map<int, int> m;
        for(int x : nums1){
            m[x]++;
        }
        vector<int> intersection;
        for(int x : nums2){
            if(m.count(x)){
                intersection.push_back(x);
                m[x]--;
                if(m[x] == 0)
                m.erase(x);
            }
        }
        return intersection;
    }
};
