class Solution {
public:
    vector<int> twoSum(vector<int>& numbers, int target) {
        int low = 0, hight = numbers.size() - 1;
        while(low < hight){
            if(numbers[low] + numbers[hight] == target) return {low + 1, hight + 1};
            else if(numbers[low] + numbers[hight] < target) low++;
            else hight--;
        }
        return {-1, -1};
    }
};
