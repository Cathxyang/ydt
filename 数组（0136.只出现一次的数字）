//unordered_set
class Solution {
public:
    int singleNumber(vector<int>& nums) {
        unordered_set<int> res(nums.size());
        for(int i = 0; i < nums.size(); i++){
            if(res.count(nums[i])) res.erase(nums[i]);
            else res.insert(nums[i]);
        }
        return *res.begin();
    }
};

//位运算
class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int res = 0;
        for(auto e : nums) res ^= e;
        return res;
    }
};
